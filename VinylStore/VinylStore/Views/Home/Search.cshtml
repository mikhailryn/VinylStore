@model SearchViewModel

<br />
<h1>@Model.Information</h1>



@if (Model.GetArtists != null)
{

    @foreach (var artist in Model.GetArtists)
    {
<div class="col-lg-4 col-md-6 mb-4">
    <div class="thumbnail">

        <img src="@artist.ImgUrl" />

        <div class="caption">
            <h3 class="pull-right">@artist.ArtistName</h3> 
            @artist.Albums

            </div>

        </div>
    </div>
    }


    }
